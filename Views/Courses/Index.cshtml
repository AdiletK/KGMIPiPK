@model IEnumerable<KGMIPiPK.Courses>

@{
    ViewData["Title"] = "Курсы";
}

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.From)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.To)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FullName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Graduated)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Plan)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Oplata)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IstFinNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.From)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.To)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FullName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Graduated)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Plan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Oplata)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IstFinNavigation.Name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Nom">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Nom">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Nom">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
